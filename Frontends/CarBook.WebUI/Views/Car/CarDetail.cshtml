
@{
    ViewData["Title"] = "CarDetail";
    Layout = "~/Views/UILayout/Index.cshtml";
}

<section class="ftco-section ftco-car-details">
	<div class="container">
		@await Component.InvokeAsync("CarDetailMainCarFuture", new {id = ViewBag.carId})
		<div class="row">
			<div class="col-md-12 pills">
				<div class="bd-example bd-example-tabs">
					@await Component.InvokeAsync("CarDetailTabPane")
					

					<div class="tab-content" id="pills-tabContent">
						
						@await Component.InvokeAsync("CarDetailSubFeature", new { id = ViewBag.carId })

						@await Component.InvokeAsync("CarDetailDescription", new { id = ViewBag.carId })

						@await Component.InvokeAsync("CarDetailComment", new { id = ViewBag.carId })
					

					</div>
				</div>
			</div>
		</div>
	</div>
</section>

