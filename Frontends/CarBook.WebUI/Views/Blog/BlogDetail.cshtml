
@{
    ViewData["Title"] = "BlogDetail";
    Layout = "~/Views/UILayout/Index.cshtml";
}

<<section class="ftco-section ftco-degree-bg">
      <div class="container">
        <div class="row">

            @await Component.InvokeAsync("BlogDetailsMain")

          <div class="col-md-4 sidebar ftco-animate">
            
              @await Component.InvokeAsync("BlogDetailSideBarSearchBox")

              @await Component.InvokeAsync("BlogDetailsCategory")

              @await Component.InvokeAsync("RecentBlogs")

              @await Component.InvokeAsync("TagCloud") 
              
              @await Component.InvokeAsync("BlogPharagraph")             
            
          </div>

        </div>
      </div>
    </section>

